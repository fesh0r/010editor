typedef struct {
	ACCESS_FLAGS access_flags;
	u2 name_index /*<read=read_name>*/;
	u2 descriptor_index /*<read=read_name>*/;
	u2 attributes_count;
	if (attributes_count) {
		attribute_info attributes[attributes_count];
	}
} method_info </*read=read_method_info,*/optimize=false>;
