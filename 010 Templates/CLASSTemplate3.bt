typedef struct {
} cp_null;

typedef struct {
	u2 attribute_name_index /*<read=read_name>*/;
	u4 attribute_length;
	if (attribute_length) {
		u1 info[attribute_length];
	}
} attribute_info </*read=read_attribute_info,*/optimize=false>;
